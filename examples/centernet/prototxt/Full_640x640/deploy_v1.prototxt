	}
	convolution_param {
		num_output: 128
		bias_term: false
		kernel_size: 3
		pad: 1
		group: 128
		weight_filler {
			type: "msra"
		}
	}
}
layer {
	name: "conv_object_size/bn"
	type: "BatchNorm"
	bottom: "conv_object_size"
	top: "conv_object_size"
	param {
		lr_mult: 0
		decay_mult: 0
	}
	param {
		lr_mult: 0
		decay_mult: 0
	}
	param {
		lr_mult: 0
		decay_mult: 0
	}
}
layer {
	name: "conv_object_size/scale"
