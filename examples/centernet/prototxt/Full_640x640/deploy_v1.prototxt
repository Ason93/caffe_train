		decay_mult: 0.0
	}
	param {
		lr_mult: 2.0
		decay_mult: 0.0
	}
	scale_param {
		filler {
			value: 1
		}
		bias_term: true
		bias_filler {
			value: 0
		}
	}
}
layer {
	name: "conv_object_size/ReLU"
	type: "ReLU"
	bottom: "conv_object_size"
	top: "conv_object_size"
}
layer{
	name: "conv_object_size_1x1"
	type: "Convolution"
	bottom: "conv_object_size"
	top: "conv_object_size_1x1"
	param {
		lr_mult: 1
		decay_mult: 1
	}
	convolution_param {
		num_output: 2
		kernel_size: 1
		#pad: 1
		weight_filler {
			type: "msra"
		}
		bias_term: false
	}
}
layer {
	name: "conv_object_size_1x1/bn"
	type: "BatchNorm"
	bottom: "conv_object_size_1x1"
	top: "conv_object_size_1x1"
	param {
		lr_mult: 0
		decay_mult: 0
	}
	param {
		lr_mult: 0
		decay_mult: 0
	}
	param {
		lr_mult: 0
		decay_mult: 0
	}
}
layer {
	name: "conv_object_size_1x1/scale"
	type: "Scale"
	bottom: "conv_object_size_1x1"
	top: "conv_object_size_1x1"
	param {
		lr_mult: 1.0
		decay_mult: 0.0
	}
	param {
		lr_mult: 2.0
		decay_mult: 0.0
	}
	scale_param {
		filler {
			value: 1
		}
		bias_term: true
		bias_filler {
			value: 0
		}
	}
}
layer {
    name: "loc_offset_conv_1x1_sigmoid"
	type: "Sigmoid"
	bottom: "loc_offset_conv_1x1"
	top: "loc_offset_conv_1x1_sigmoid"
}
layer {
  name: "prediction_concate_output"
  type: "Concat"
  bottom: "loc_offset_conv_1x1_sigmoid"
  bottom: "conv_object_size_1x1"
  top: "prediction_concate_output"
}
layer {
    name: "conf_heatmap_64_1x1_sigmoid"
	type: "Sigmoid"
	bottom: "conf_heatmap_64_1x1"
	top: "conf_heatmap_64_1x1_sigmoid"
}
layer {
    name: "conf_heatmap_64_1x1_pool"
	type: "Pooling"
    bottom: "conf_heatmap_64_1x1_sigmoid"
    top: "conf_heatmap_64_1x1_pool"
    pooling_param {
        pool: MAX
        kernel_size: 3
        stride: 1
        pad: 1
    }
}


layer {
	name: "detection_out"
	type: "CenternetDetectionOutput"
	bottom: "conf_heatmap_64_1x1"
	bottom: "conf_heatmap_64_1x1_pool"
	bottom: "prediction_concate_output"
	top: "detection_out"
	include {
	phase: TEST
	}
	detection_output_param {
		num_classes: 2
		share_location: true
		keep_top_k: 100
		confidence_threshold: 0.25
	}
}