	}
}
layer {
	name: "centernet_loss"
	type: "CenterObjectLoss"
	bottom: "conv_loc_1x1"
	bottom: "conf_heatmap_1x1"
	bottom: "label"
	top: "centernet_loss"
	include {
		phase: TRAIN
	}
	propagate_down: true
	propagate_down: true
	propagate_down: false
	loss_param {
		normalization: VALID
	}
	loss_weight: 1.0
	center_object_loss_param {
		loc_loss_type: SMOOTH_L1
		conf_loss_type: FOCALSIGMOID
		loc_weight: 1.0
		num_class: 1
		share_location: true
	}
}