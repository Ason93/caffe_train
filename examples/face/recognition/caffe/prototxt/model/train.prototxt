##############cosinface ##################
layer {
	name: "lmcl_cos_l2_normalize"
	type: "CosinL2Normalize"
	bottom: "fc5"
	bottom: "labelIndex"
	top: "lmcl_cos_l2_normalize"
	param {
		lr_mult: 1
		decay_mult: 2 
	}
	cosin_loss_param {
		margin: -0.35
		scale: 64
		num_output: 8631
		cosin_filler {
			type: "xavier"
		}
	}
}
