  bottom: "classifier"
  bottom: "label"
  top: "loss"
}

layer {
	name: "merege_pair"
	type: "Identity2Verify"
	bottom: "fc5"
	bottom: "label"
	top: "feature1"
	top: "feature2"
	top: "sim"
}

layer {
	name: "norm1"
	type: "SimpleNormalize"
	bottom: "feature1"
	top: "normfeature1"
}

layer {
	name: "norm2"
	type: "SimpleNormalize"
	bottom: "feature2"
	top: "normfeature2"
}

layer {
  name: "loss2"
  type: "ContrastiveLoss"
  bottom: "normfeature1"
  bottom: "normfeature2"
  bottom: "sim"
  top: "sim_loss"
  loss_weight: 0.3
  contrastive_loss_param {
    margin: 1
  }
}
############## center loss ###############
layer {
  name: "center_loss"
  type: "CenterLoss"
  bottom: "fc5"
  bottom: "label"
  top: "center_loss"
  param {
    lr_mult: 1
    decay_mult: 2 
  }
  center_loss_param {
    num_output: 8631
    center_filler {
      type: "xavier"
    }
  }
  loss_weight: 0.008
}

##########################################
layer {
  name: "lmcl_cos_l2_normalize"
  type: "CosinL2Normalize"
  bottom: "fc5"
  bottom: "label"
  top: "lmcl_cos_l2_normalize"
  param {
    lr_mult: 1
    decay_mult: 2 
  }
  cosin_loss_param {
    margin: -0.35
    scale: 30
    num_output: 8631
    cosin_filler {
      type: "xavier"
    }
  }
  propagate_down: true
}
layer {
  name: "cosin_loss"
  type: "SoftmaxWithLoss"
  bottom: "lmcl_cos_l2_normalize"
  bottom: "label"
  top: "cosin_loss"
  propagate_down: true
  propagate_down: false
}

