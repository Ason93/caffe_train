  }
}
layer {
  name: "multiface_output"
  type: "Concat"
  bottom: "conv6_landmark"
  bottom: "conv6_gender"
  bottom: "conv6_glasses"
  bottom: "conv6_headpose"
  top: "multiface_output"
  concat_param {
		axis: 1
  }
  include {
		phase: TEST
	}
}
